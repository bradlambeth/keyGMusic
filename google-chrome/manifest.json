{
  "name": "keySharky",
  "version": "1.3",
  "description": "Add missing keyboard functionality to Grooveshark!",
  "background_page": "keysharky.html",
  "options_page": "options.html",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches" : ["http://*/*", "https://*/*", "ftp://*/*"],
      "js" : ["listener.js"],
      "run_at" : "document_end"
    }
  ],
  "icons" : {
    "48"  : "gs48.png",
    "128" : "gs128.png"
  }
}
