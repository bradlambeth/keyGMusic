{
  "name": "keyGMusic",
  "version": "0.2",
  "description": "Add missing keyboard functionality to Google Music Player!",
  "background_page": "keygmusic.html",
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "*://music.google.com/*"
  ],
  "content_scripts": [
    {
      "matches" : ["*://music.google.com/*"],
      "js" : ["listener.js"],
      "run_at" : "document_end"
    }
  ],
  "icons" : {
    "48"  : "keygmusic-48.png",
    "128" : "keygmusic-128.png"
  }
}
